/* pages/customerService/customerService.wxss */
/* 客服样式 */
page {
  background-color: #f0f0f0;
}
.container {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
  text-align: center;
  padding: 400rpx 0 0 90rpx;
}
.item {
  width: 48%;
  height: 200rpx;
  margin: 0 0 -100rpx -50rpx;
}
image {
  width: 75rpx;
  height: 70rpx;
}
text {
  font-size: 26rpx;
  color: #666;
  position: relative;
  top: -25rpx;
  left: 0;
}